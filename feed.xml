---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
<title>{{ site.name }}</title>
<description>{{ site.description }}</description>
<link>http://{{ site.url }}</link>
<atom:link href="{{ site.url }}/feed.posts.xml" rel="self" type="application/rss+xml" />
{% for post in site.categories.postitused %}
{% unless post.link %}
<item>
<title>{{ post.title }}</title>
<description>{{ post.content | xml_escape }}</description>
<pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
<link>http://{{ site.url }}{{ post.url }}</link>
<guid isPermaLink="true">http://{{ site.url }}{{ post.url }}</guid>
</item>
{% endunless %}
{% endfor %}
</channel>
</rss>
